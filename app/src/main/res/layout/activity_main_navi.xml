<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    android:id="@+id/tablayout_navi">


    <ImageView
        android:id="@+id/img_home_main_navi_act"
        android:layout_width="20dp"
        android:layout_height="17dp"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/img_search_main_navi_act"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/home"/>


    <ImageView
        android:id="@+id/img_search_main_navi_act"
        android:layout_width="17.5dp"
        android:layout_height="17.5dp"
        app:layout_constraintEnd_toStartOf="@id/img_course_main_navi_act"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_home_main_navi_act"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/search"
        />

    <ImageView
        android:id="@+id/img_course_main_navi_act"
        android:layout_width="18dp"
        android:layout_height="18dp"
        app:layout_constraintEnd_toStartOf="@id/img_course_recommend_main_navi_act"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_search_main_navi_act"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/mode_edit" />

    <ImageView
        android:id="@+id/img_course_recommend_main_navi_act"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintEnd_toStartOf="@id/img_mypage_main_navi_act"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_course_main_navi_act"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/library_music"/>

    <ImageView
        android:id="@+id/img_mypage_main_navi_act"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/img_course_recommend_main_navi_act"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/person"/>


</androidx.constraintlayout.widget.ConstraintLayout>